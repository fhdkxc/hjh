<script>
var clasCode = "";

// 권한(학생, 교직원, 부모)에 따라 마이페이지 변경
const myPageHrefChange = function(p){
	console.log("p",p);
	
	if(isStudent()){
		p.href = "/student/mypage";
	}
	if(isParent()){
		p.href = "/parents/mypage";
	}
	if(isEmployee()){
		p.href = "/teacher/mypage";
	}
}

// 권한별로 학급 클래스 페이지로 이동
const headerClassList = function(){
	// 학생 or 교사일 때  현재 소속된 클래스로 이동 
	<sec:authorize access="hasRole('A01001') or hasRole('A01002')">
	// 클래스 코드 가져오기
	$.ajax({
		url:"/header/getClasCode",
		type:"post",
		dataType:"text",
		beforeSend:function(xhr){
			xhr.setRequestHeader("${_csrf.headerName}","${_csrf.token}");
		},
		success:function(clasCode){
			console.log("clasCode -> " + clasCode);

			$("#clasCode").val(clasCode);
			$("#goToClassForm").submit();
		}
	});
	
	</sec:authorize>
	
	// 학부모일 때 자녀의 학급 클래스 목록 모달 창 띄우기
	<sec:authorize access="hasRole('A01003')">
	$("#goToChildClassModal").modal("show");
	
	$.ajax({
		url:"/myClassList",
		method:"post",
		dataType:"json",
		beforeSend:function(xhr){
			xhr.setRequestHeader("${_csrf.headerName}","${_csrf.token}");
		},
		success:function(res){
			var str = "";
			var clasCode = ""; 		// 학생, 교사의 클래스 코드
			var clasCodeArr = []; 	// 학부모 자녀의 클래스 코드들
			console.log("res",  res);
			
			res.forEach(function(item, index){
				console.log("클래스 코드 -> " + item.clasCode);
				clasCode = item.clasCode;
				
				clasCodeArr[index] = item.clasCode;
				console.log("클래스 코드 배열 -> ", clasCodeArr);
				
				str += "<div class='col-lg-6 col-md-6 col-sm-6 col-xs-12'>";
				str += "<div class='hpanel hblue contact-panel contact-panel-cs responsive-mg-b-30'>";
				str += "<div class='panel-body custom-panel-jw child-info'>";
				str += "<h3>"+item.memberVO.mberNm+"</h3>"
				str += "<p>"+item.schulVO.schulNm+"</p>"
				str += "<p>"+item.clasNm+"</p>"
				str += "<p>"+item.clasCode+"</p>"
				str += "</div>";
				 str += "<input type='button' onclick='goToClass(\"" + item.clasCode + "\")' class='btn btn-primary waves-effect waves-light' value='이동하기'>";
				str += "</div></div>";
				
// 				$("#clasCode1").val(item.clasCode);
			});
			
			$(".child-content").html(str);
		}
	});
	</sec:authorize>
};

const goToClass = function(clasCode){
	document.querySelector("#clasCode").value = clasCode;
	document.querySelector("#goToClassForm").submit();
};
</script>

