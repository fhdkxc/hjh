					<otherwise>TO_CHAR(RQST_DE, 'YYYY-MM-DD') BETWEEN #{rqstDeStartDate} AND #{rqstDeEndDate}</otherwise>
				</choose>
			)
			</if>
			AND (
			    <foreach collection="cmmnProcessSttusCB" item="sttus" index="index" separator=" OR ">
			        <choose>
			            <when test="sttus == 'sttusSuccess'">
			                (CMMN_PROCESS_STTUS = 'A11002')
			            </when>
			            <when test="sttus == 'sttusWait'">
			                (CMMN_PROCESS_STTUS = 'A11001')
			            </when>
			            <when test="sttus == 'sttusCompensate'">
			            	(CMMN_PROCESS_STTUS = 'A11003')
			            </when>
			            <when test="sttus == 'sttusDeny'">
			            	(CMMN_PROCESS_STTUS = 'A11004')
			            </when>
			            <when test="sttus == 'sttusIng'">
			            	(CMMN_PROCESS_STTUS = 'A11005')
			            </when>
			            <otherwise>
			                (  CMMN_PROCESS_STTUS = 'A11001' 
			                OR CMMN_PROCESS_STTUS = 'A11002' 
			                OR CMMN_PROCESS_STTUS = 'A11003' 
			                OR CMMN_PROCESS_STTUS = 'A11004' 
			                OR CMMN_PROCESS_STTUS = 'A11005'
			                )
			            </otherwise>
			        </choose> 
			    </foreach>
			)
		    <if test="keyword != null || keyword == ''">
		    AND (
		    	<choose>
		    		<when test="searchCondition == 'purps'"> PURPS LIKE '%' || #{keyword} || '%' </when>
		    		<when test="searchCondition == 'dstn'">  DSTN LIKE '%' || #{keyword} || '%' </when>
		    		<otherwise>
		    		(
		    				PURPS LIKE '%' || #{keyword} || '%'
		    			OR	DSTN LIKE '%' || #{keyword} || '%'
		    		)
		    		</otherwise>
		    	</choose>
		    )
		    </if>		  
	</select>
	
	<!-- 학부모-체험학습 문서 목록 상세 -->
	<select id="approvalDetail" parameterType="String" resultMap="sanctnDocMap">
		/* kr.or.ddit.approval.mapper.approvalDetail */
		SELECT S.SCHUL_NM
		    , H.CLAS_NM
		    , H.CMMN_GRADE
		    , H.CLAS_IN_NO
		    , H.CLAS_CODE
		    , H.STDNPRNT_NM
		    , H.MOBLPHON_NO
		    , H.STDNT_NM
		    , H.DOC_CODE
		    , H.STDNT_ID
		    , H.EXPRN_LRN_BGNDE
		    , H.LRN_STLE
		    , H.PURPS
		    , H.DSTN
		    , H.STDNPRNT_ID
		    , H.DOC_CN
		    , H.RQST_DE
		    , H.SCHUL_CODE
		    , H.TCHER_SANCTN
		    , H.TCHER_ID
		    , H.DEPUTY_PRNCPAL_SANCTN
		    , H.DEPUTY_PRNCPAL_ID
		    , H.CMMN_DOC_KND
		    , H.CLAS_STDNT_CODE
		    , H.CMMN_PROCESS_STTUS
		    , H.DOC_REJECT
		    , H.ATCH_FILE_CODE
		    , H.ATCH_FILE_SN
		    , H.EXPRN_LRN_ENDDE
		    , FN_CMMN_CODE_TO_NM(H.CMMN_DETAIL_CODE) CMMN_DETAIL_CODE
		FROM SCHUL S
		INNER JOIN
		    (SELECT G.CLAS_NM
		        , G.CMMN_GRADE
		        , G.CLAS_IN_NO
		        , G.CLAS_CODE
		        , G.STDNPRNT_NM
		        , G.MOBLPHON_NO
		        , G.STDNT_NM
		        , G.DOC_CODE
		        , G.STDNT_ID
		        , G.EXPRN_LRN_BGNDE
		        , G.LRN_STLE
		        , G.PURPS
		        , G.DSTN
		        , G.STDNPRNT_ID
		        , G.DOC_CN
		        , G.RQST_DE
		        , G.SCHUL_CODE
		        , G.TCHER_SANCTN
		        , G.TCHER_ID
		        , G.DEPUTY_PRNCPAL_SANCTN
		        , G.DEPUTY_PRNCPAL_ID
		        , G.CMMN_DOC_KND
		        , G.CLAS_STDNT_CODE
		        , G.CMMN_PROCESS_STTUS
		        , G.DOC_REJECT
		        , G.ATCH_FILE_CODE
		        , G.ATCH_FILE_SN
		        , G.EXPRN_LRN_ENDDE
		        , G.CMMN_DETAIL_CODE
		    FROM ATCH_FILE AF
		    RIGHT OUTER JOIN
		        (SELECT F.CLAS_NM
		            , F.CMMN_GRADE
		            , F.CLAS_IN_NO
		            , F.CLAS_CODE
		            , F.STDNPRNT_NM
		            , F.MOBLPHON_NO
		            , F.STDNT_NM
		            , F.DOC_CODE
		            , F.STDNT_ID
		            , F.EXPRN_LRN_BGNDE
		            , F.LRN_STLE
		            , F.PURPS
		            , F.DSTN
		            , F.STDNPRNT_ID
		            , F.DOC_CN
		            , F.RQST_DE
		            , F.SCHUL_CODE
		            , F.TCHER_SANCTN
		            , F.TCHER_ID
		            , F.DEPUTY_PRNCPAL_SANCTN
		            , F.DEPUTY_PRNCPAL_ID
		            , F.CMMN_DOC_KND
		            , F.CLAS_STDNT_CODE
		            , F.CMMN_PROCESS_STTUS
		            , F.DOC_REJECT
		            , F.ATCH_FILE_CODE
		            , F.ATCH_FILE_SN
		            , F.EXPRN_LRN_ENDDE
		            , F.CMMN_DETAIL_CODE
		        FROM SCHUL_PSITN_MBER SPM2
		        RIGHT OUTER JOIN
		            (SELECT E.CLAS_NM
		            , E.CMMN_GRADE
		            , E.CLAS_IN_NO
		            , E.CLAS_CODE
		            , E.STDNPRNT_NM
		            , E.MOBLPHON_NO
		            , E.STDNT_NM
		            , E.DOC_CODE
		            , E.STDNT_ID
		            , E.EXPRN_LRN_BGNDE
		            , E.LRN_STLE
		            , E.PURPS
		            , E.DSTN
		            , E.STDNPRNT_ID
		            , E.DOC_CN
		            , E.RQST_DE
		            , E.SCHUL_CODE
		            , E.TCHER_SANCTN
		            , E.TCHER_ID
		            , E.DEPUTY_PRNCPAL_SANCTN
		            , E.DEPUTY_PRNCPAL_ID
		            , E.CMMN_DOC_KND
		            , E.CLAS_STDNT_CODE
		            , E.CMMN_PROCESS_STTUS
		            , E.DOC_REJECT
		            , E.ATCH_FILE_CODE
		            , E.ATCH_FILE_SN
		            , E.EXPRN_LRN_ENDDE
		            , E.CMMN_DETAIL_CODE
		            FROM SCHUL_PSITN_MBER SPM1
		            RIGHT OUTER JOIN
		                (SELECT CL.CLAS_NM
		                        , CL.CMMN_GRADE
		                        , D.CLAS_IN_NO
		                        , D.CLAS_CODE
		                        , D.STDNPRNT_NM
		                        , D.MOBLPHON_NO
		                        , D.STDNT_NM
		                        , D.DOC_CODE
		                        , D.STDNT_ID
		                        , D.EXPRN_LRN_BGNDE
		                        , D.LRN_STLE
		                        , D.PURPS
		                        , D.DSTN
		                        , D.STDNPRNT_ID
		                        , D.DOC_CN
		                        , D.RQST_DE
		                        , D.SCHUL_CODE
		                        , D.TCHER_SANCTN
		                        , D.TCHER_ID
		                        , D.DEPUTY_PRNCPAL_SANCTN
		                        , D.DEPUTY_PRNCPAL_ID
		                        , D.CMMN_DOC_KND
		                        , D.CLAS_STDNT_CODE
		                        , D.CMMN_PROCESS_STTUS
		                        , D.DOC_REJECT
		                        , D.ATCH_FILE_CODE
		                        , D.ATCH_FILE_SN
		                        , D.EXPRN_LRN_ENDDE
		                        , D.CMMN_DETAIL_CODE
		                    FROM CLAS CL
		                    INNER JOIN
		                    (SELECT CS.CLAS_IN_NO
		                            , CS.CLAS_CODE
		                            , C.STDNPRNT_NM
		                            , C.MOBLPHON_NO
		                            , C.STDNT_NM
		                            , C.DOC_CODE
		                            , C.STDNT_ID
		                            , C.EXPRN_LRN_BGNDE
		                            , C.LRN_STLE
		                            , C.PURPS
		                            , C.DSTN
		                            , C.STDNPRNT_ID
		                            , C.DOC_CN
		                            , C.RQST_DE
		                            , C.SCHUL_CODE
		                            , C.TCHER_SANCTN
		                            , C.TCHER_ID
		                            , C.DEPUTY_PRNCPAL_SANCTN
		                            , C.DEPUTY_PRNCPAL_ID
		                            , C.CMMN_DOC_KND
		                            , C.CLAS_STDNT_CODE
		                            , C.CMMN_PROCESS_STTUS
		                            , C.DOC_REJECT
		                            , C.ATCH_FILE_CODE
		                            , C.ATCH_FILE_SN
		                            , C.EXPRN_LRN_ENDDE
		                            , C.CMMN_DETAIL_CODE
		                         FROM CLAS_STDNT CS
		                         INNER JOIN
		                        (SELECT M2.MBER_NM STDNPRNT_NM
		                            , M2.MOBLPHON_NO 
		                            , B.STDNT_NM
		                            , B.DOC_CODE
		                            , B.STDNT_ID
		                            , B.EXPRN_LRN_BGNDE
		                            , B.LRN_STLE
		                            , B.PURPS
		                            , B.DSTN
		                            , B.STDNPRNT_ID
		                            , B.DOC_CN
		                            , B.RQST_DE
		                            , B.SCHUL_CODE
		                            , B.TCHER_SANCTN
		                            , B.TCHER_ID
		                            , B.DEPUTY_PRNCPAL_SANCTN
		                            , B.DEPUTY_PRNCPAL_ID
		                            , B.CMMN_DOC_KND
		                            , B.CLAS_STDNT_CODE
		                            , B.CMMN_PROCESS_STTUS
		                            , B.DOC_REJECT
		                            , B.ATCH_FILE_CODE
		                            , B.ATCH_FILE_SN
		                            , B.EXPRN_LRN_ENDDE
		                            , B.CMMN_DETAIL_CODE
		                        FROM MEMBER M2
		                        INNER JOIN
		                            (SELECT M1.MBER_NM STDNT_NM
		                                 , A.DOC_CODE
		                                 , A.STDNT_ID
		                                 , A.EXPRN_LRN_BGNDE
		                                 , A.LRN_STLE
		                                 , A.PURPS
		                                 , A.DSTN
		                                 , A.STDNPRNT_ID
		                                 , A.DOC_CN
		                                 , A.RQST_DE
		                                 , A.SCHUL_CODE
		                                 , A.TCHER_SANCTN
		                                 , A.TCHER_ID
		                                 , A.DEPUTY_PRNCPAL_SANCTN
		                                 , A.DEPUTY_PRNCPAL_ID
		                                 , A.CMMN_DOC_KND
		                                 , A.CLAS_STDNT_CODE
		                                 , A.CMMN_PROCESS_STTUS
		                                 , A.DOC_REJECT
		                                 , A.ATCH_FILE_CODE
		                                 , A.ATCH_FILE_SN
		                                 , A.EXPRN_LRN_ENDDE
		                                 , A.CMMN_DETAIL_CODE
		                            FROM MEMBER M1
		                            INNER JOIN
		                                (SELECT SD.DOC_CODE
		                                         , SD.STDNT_ID
		                                         , SD.EXPRN_LRN_BGNDE
		                                         , SD.LRN_STLE
		                                         , SD.PURPS
		                                         , SD.DSTN
		                                         , SD.STDNPRNT_ID
		                                         , SD.DOC_CN
		                                         , SD.RQST_DE
		                                         , SD.SCHUL_CODE
		                                         , SD.TCHER_SANCTN
		                                         , SD.TCHER_ID
		                                         , SD.DEPUTY_PRNCPAL_SANCTN
		                                         , SD.DEPUTY_PRNCPAL_ID
		                                         , SD.CMMN_DOC_KND
		                                         , SD.CLAS_STDNT_CODE
		                                         , SD.CMMN_PROCESS_STTUS
		                                         , SD.DOC_REJECT
		                                         , SD.ATCH_FILE_CODE
		                                         , SD.ATCH_FILE_SN
		                                         , SD.EXPRN_LRN_ENDDE
		                                         , FR.CMMN_DETAIL_CODE
		                                    FROM SANCTN_DOC SD 
		                                    INNER JOIN FAMILY_RELATE FR 
		                                    ON SD.STDNT_ID = FR.STDNT_ID
		                                    AND SD.STDNPRNT_ID = FR.STDNPRNT_ID
		                                    AND SD.SCHUL_CODE = FR.SCHUL_CODE
		                                    WHERE SD.DOC_CODE = #{docCode}) A
		                            ON M1.MBER_ID = A.STDNT_ID) B
		                        ON M2.MBER_ID = B.STDNPRNT_ID) C
		                    ON CS.CLAS_STDNT_CODE = C.CLAS_STDNT_CODE) D
		                ON CL.CLAS_CODE = D.CLAS_CODE) E
		            ON E.TCHER_ID = SPM1.MBER_ID) F
		        ON F.DEPUTY_PRNCPAL_ID = SPM2.MBER_ID) G
		    ON AF.ATCH_FILE_CODE = G.ATCH_FILE_CODE
		    AND AF.ATCH_FILE_SN = G.ATCH_FILE_SN) H
		ON S.SCHUL_CODE = H.SCHUL_CODE
	</select>
	
	<!-- 학부모-체험학습 문서 수정 -->
	<update id="approvalUpdate" parameterType="sanctnDocVO">
		/* kr.or.ddit.approval.mapper.approvalUpdate */
		UPDATE SANCTN_DOC
		   SET EXPRN_LRN_BGNDE = #{exprnLrnBgnde}
		     , EXPRN_LRN_ENDDE = #{exprnLrnEndde}
		     , LRN_STLE = #{lrnStle}
		     , PURPS = #{purps}
		     , DSTN = #{dstn}
		     , DOC_CN = #{docCn}
		     , RQST_DE = #{rqstDe}
		 WHERE DOC_CODE = #{docCode}
	</update>
	
	<!-- 학부모-체험학습 문서 삭제 -->
	<delete id="approvalDelete" parameterType="sanctnDocVO">
		/* kr.or.ddit.approval.mapper.approvalDelete */
		DELETE FROM SANCTN_DOC WHERE DOC_CODE = #{docCode}		
	</delete>
	
</mapper>