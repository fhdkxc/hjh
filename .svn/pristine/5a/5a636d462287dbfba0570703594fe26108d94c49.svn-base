<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.school.mapper.SchafsSchedulMapper">
	<!-- 학사 일정 등록 -->
	<insert id="scheduleInsert" parameterType="schafsSchdulVO">
	</insert>
	
	<!-- 학사 일정 리스트 -->
	<select id="scheduleList" resultType="schafsSchdulVO">
		SELECT
		SCHAFS_SCHDUL_CODE, SCHAFS_SCHDUL_NM, SCHAFS_SCHDUL_CN, SCHAFS_SCHDUL_BGNDE, SCHAFS_SCHDUL_ENDDE
		FROM SCHAFS_SCHDUL
	</select>
	
	<!-- 학사 일정 삭제 -->
	<delete id="scheduleDelete" parameterType="String">
	</delete>
	
	<!-- 학사 일정 수정 -->
	<update id="scheduleUpdate" parameterType="schafsSchdulVO">
	</update>
	
</mapper>